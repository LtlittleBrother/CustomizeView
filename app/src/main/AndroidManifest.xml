<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.customizeview">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
                android:name=".activity.CustomizeViewTwo"
                android:screenOrientation="portrait"/>
        <activity
                android:name=".activity.TimeSelectorActivity"
                android:screenOrientation="portrait"/>
        <activity android:name=".activity.ImageViewActivity"/>
        <activity android:name=".activity.CustomizeViewThree"/>
        <activity android:name=".activity.CustomizeViewFour"/>
        <activity android:name=".activity.CustomizeViewSix"/>
        <activity android:name=".activity.CustomizeViewSeven"/>
        <activity android:name=".activity.PraiseActivity" />
    </application>

</manifest>