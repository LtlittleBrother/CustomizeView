<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

    <TextView
        android:id="@+id/title_tv"
        android:layout_width="match_parent"
        android:layout_height="@dimen/game_center_dimen_50_dip"
        android:textSize="@dimen/sp_15"
        android:textColor="@color/dianyou_color_222222"
        android:gravity="center"/>

    <com.customizeview.view1.Practice01DrawColorView
        android:id="@+id/draw_color_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"/>

    <com.customizeview.view1.Practice02DrawCircleView
        android:id="@+id/draw_circle_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"/>

    <com.customizeview.view1.Practice03DrawRectView
        android:id="@+id/draw_rect_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"/>

    <com.customizeview.view1.Practice04DrawPointView
        android:id="@+id/draw_point_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"/>

    <com.customizeview.view1.Practice05DrawOvalView
        android:id="@+id/draw_oval_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"/>

    <com.customizeview.view1.Practice06DrawLineView
        android:id="@+id/draw_line_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"/>

    <com.customizeview.view1.Practice07DrawRoundRectView
        android:id="@+id/draw_round_rect_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"/>

    <com.customizeview.view1.Practice08DrawArcView
        android:id="@+id/draw_arc_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"/>

    <com.customizeview.view1.Practice09DrawPathView
        android:id="@+id/draw_path_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"/>

    <com.customizeview.view1.Practice10DrawHistogramView
        android:id="@+id/draw_histogram_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"/>

    <com.customizeview.view1.Practice11DrawPieChartView
        android:id="@+id/draw_pie_chart_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"/>


</LinearLayout>